    // calendar modal
    #CalenderModalNew.modal.fade(tabindex='-1', role='dialog', aria-labelledby='myModalLabel', aria-hidden='true')
      .modal-dialog
        .modal-content
          .modal-header
            button.close(type='button', data-dismiss='modal', aria-hidden='true') ×
            h4#myModalLabel.modal-title New Calendar Entry
          .modal-body
            #testmodal(style='padding: 5px 20px;')
              form#antoform.form-horizontal.calender(role='form')
                .form-group
                  label.col-sm-3.control-label Title
                  .col-sm-9
                    input#title.form-control(type='text', name='title')
                .form-group
                  label.col-sm-3.control-label Description
                  .col-sm-9
                    textarea#descr.form-control(style='height:55px;', name='descr')
          .modal-footer
            button.btn.btn-default.antoclose(type='button', data-dismiss='modal') Close
            button.btn.btn-primary.antosubmit(type='button') Save changes
    #CalenderModalEdit.modal.fade(tabindex='-1', role='dialog', aria-labelledby='myModalLabel', aria-hidden='true')
      .modal-dialog
        .modal-content
          .modal-header
            button.close(type='button', data-dismiss='modal', aria-hidden='true') ×
            h4#myModalLabel2.modal-title Edit Calendar Entry
          .modal-body
